<Window x:Class="RentACarDesktop.MainPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RentACarDesktop"
        mc:Ignorable="d"
        Title="RentACar Admin - Main Panel" WindowStartupLocation="CenterScreen" Height="480" Width="800">
    <Grid>
        <Label Margin="30,30,0,0" FontSize="32" Content="Choose what would you like to edit" />
        <Button x:Name="btnUsers"
                    Height="150" Margin="468,175,30,110" FontWeight="Bold" FontSize="32" Content="Users" Click="btnUsers_Click"/>
        <Button x:Name="btnVehicles"
                    Height="150" Margin="30,175,469,110" FontWeight="Bold" FontSize="32" Content="Vehicles" Click="btnVehicles_Click"/>
    </Grid>
</Window>
